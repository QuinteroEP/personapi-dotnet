@model personapi_dotnet.Models.Entities.Estudio

<h1>Editar Estudio</h1>
<form asp-action="Edit" asp-controller="Estudio" method="post">
 @Html.AntiForgeryToken()
 <input type="hidden" name="CcPer" value="@Model.CcPer" />
 <div class="form-group">
 <label for="IdProf">Id Profesion:</label>
 <input type="number" class="form-control" id="IdProf" name="IdProf" value="@Model.IdProf" required />
 </div>
 <div class="form-group">
 <label for="Univer">Universidad:</label>
 <input type="text" class="form-control" id="Univer" name="Univer" value="@Model.Univer" />
 </div>
 <div class="form-group">
 <label for="Fecha">Fecha:</label>
 <input type="date" class="form-control" id="Fecha" name="Fecha" value="@Model.Fecha" />
 </div>
 <button type="submit" class="btn btn-primary">Guardar</button>
</form>